{
  "name": "midi-playground",
  "description": "A playground for MIDI input devices using WebMIDI",
  "type": "module",
  "version": "0.0.1",
  "author": "Tu Tak Tran <tak@forestinthetree.com> (https://www.forestinthetree.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/forestinthetree/midi-playground.git"
  },
  "license": "AGPL-3.0-or-later",
  "scripts": {
    "dev": "dotenvx run -f .env.dev -- astro dev --port=7200 --host",
    "start": "dotenvx run -f .env.dev -- astro dev",
    "build": "astro check && dotenvx run -f .env.dev -- astro build",
    "preview": "dotenvx run -f .env.dev -- astro preview",
    "astro": "astro",
    "biome:check": "biome check",
    "biome:fix": "biome check --write"
  },
  "dependencies": {
    "@astrojs/check": "^0.8.2",
    "@astrojs/react": "^3.6.1",
    "@dotenvx/dotenvx": "^1.6.4",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.1.3",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "astro": "^4.13.2",
    "clsx": "^2.1.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.5.4",
    "vite-plugin-mkcert": "^1.17.5"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "sass-embedded": "^1.79.5"
  }
}
